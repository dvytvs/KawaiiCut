{
  "name": "KawaiiCut",
  "version": "1.0.0",
  "type": "module",
  "description": "A video editing application similar to Clipchamp",
  "main": "main.cjs",
  "scripts": {
    "dev": "vite",
    "start": "concurrently \"vite\" \"wait-on tcp:3000 && electron .\"",
    "build": "tsc && vite build",
    "dist": "npm run build && electron-builder",
    "preview": "vite preview",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "video-editor",
    "electron",
    "react",
    "clipchamp-clone"
  ],
  "homepage": "https://github.com/dvytvs/KawaiiCut",
  "author": "dvytvs <zapretsystem445@proton.me>",
  "license": "MIT",
  "build": {
    "appId": "com.kawaiicut.app",
    "productName": "KawaiiCut",
    "linux": {
      "icon": "build/icons/linux",
      "target": [
        "deb",
        "pacman",
        "snap",
        "tar.gz",
        "AppImage"
      ],
      "category": "Video"
    },
    "files": [
      "dist/**/*",
      "main.cjs"
    ],
    "directories": {
      "buildResources": "build",
      "output": "release"
    }
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@tailwindcss/vite": "^4.2.0",
    "idb": "^8.0.0",
    "lucide-react": "^0.562.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^4.2.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "typescript": "^5.2.2",
    "vite": "^6.0.0",
    "wait-on": "^7.2.0"
  }
}
